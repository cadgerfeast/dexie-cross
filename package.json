{
  "name": "dexie-cross",
  "type": "module",
  "version": "0.0.2",
  "description": "Dexie Cross Domain Addon",
  "keywords": [
    "dexie-cross",
    "dexie",
    "dexiejs",
    "database",
    "cross",
    "cors"
  ],
  "author": "Jérémie Pichon <contact@cadgerfeast.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/dts/index.d.ts"
    }
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/dts/index.d.ts",
  "scripts": {
    "dev": "madoc dev",
    "build": "run-s build:*",
    "build:lib": "run-p build:lib:*",
    "build:lib:esm": "tsc -p tsconfig.esm.json",
    "build:lib:cjs": "tsc -p tsconfig.cjs.json",
    "build:lib:dts": "tsc -p tsconfig.dts.json",
    "build:docs": "madoc build",
    "test": "jest",
    "lint": "eslint ./src ./test"
  },
  "devDependencies": {
    "@madoc/core": "^0.1.8",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vueuse/rxjs": "^9.8.2",
    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "dexie": "^3.2.2"
  }
}
